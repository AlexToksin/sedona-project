footer{
  width 100%
  padding 30px
  display flex
  flex-direction column
  align-items center
}

@media (min-width: 768px){
  footer{
    flex-direction row
    justify-content space-between
    padding-left 60px
    padding-right 60px
  }
}

@media (min-width: 1200px){
  footer{
    padding-left 130px
  }
}
